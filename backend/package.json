{
  "name": "virtual-vacation-backend",
  "version": "1.0.0",
  "description": "Backend API for Virtual Vacation application",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --ci --reporters=default --reporters=jest-junit --watchAll=false",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "security:audit": "npm audit --audit-level=moderate",
    "deps:check": "npm outdated",
    "clean": "rm -rf dist coverage"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "pg": "^8.11.3",
    "redis": "^4.6.8",
    "axios": "^1.5.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "joi": "^17.9.2",
    "express-rate-limit": "^6.10.0",
    "socket.io": "^4.7.2",
    "node-cron": "^3.0.2",
    "multer": "^2.0.2",
    "sharp": "^0.32.5",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.9",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/morgan": "^1.9.5",
    "@types/compression": "^1.7.2",
    "@types/pg": "^8.10.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/bcryptjs": "^2.4.3",
    "@types/multer": "^2.0.0",
    "@types/jest": "^29.5.5",
    "@types/supertest": "^2.0.12",
    "typescript": "^5.2.2",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "jest": "^29.6.4",
    "jest-junit": "^16.0.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "prettier": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0"
  },
  "keywords": [
    "virtual",
    "travel",
    "vacation",
    "api",
    "typescript",
    "express"
  ],
  "author": "Virtual Vacation Team",
  "license": "MIT"
}
